{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS",
      "installCommand": "cd scripts/health-checks && npm install",
      "buildCommand": "cd scripts/health-checks && npm run build"
    },
    "deploy": {
      "startCommand": "cd scripts/health-checks && npm run start",
      "healthcheckPath": "/",
      "restartPolicyType": "ALWAYS"
    }
  }
  